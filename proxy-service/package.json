
{
  "name": "pcrm-proxy-service",
  "version": "4.0.0",
  "description": "Railway-optimized proxy service for PCRM - Ultra minimal v4",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "railway:start": "node server.js",
    "railway:build": "echo 'Railway build v4.0 complete'",
    "test": "echo 'No tests - minimal deployment'"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "railway": {
    "healthcheckPath": "/",
    "healthcheckTimeout": 10,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 2
  },
  "keywords": ["railway", "proxy", "minimal", "express"],
  "author": "PCRM Team",
  "license": "MIT"
}
